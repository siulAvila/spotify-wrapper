{
  "name": "spotify-wrapper",
  "version": "1.0.0",
  "description": "A wrapper to work with the spotify API",
  "main": "index.js",
  "scripts": {
    "start": "npx babel-node src/main.js",
    "test": "jasmine",
    "lint": "npx eslint src/*js --fix",
    "coverage": "npx nyc npm run test"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run coverage"
    }
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "spec/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siulAvila/Js-TDD.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Luis Fernando de Avila",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siulAvila/Js-TDD/issues"
  },
  "homepage": "https://github.com/siulAvila/Js-TDD#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "jasmine": "^3.5.0",
    "node-fetch": "^2.6.0",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "dotenv": "^8.2.0"
  }
}
